                            <td><dxe:ASPxImage EnableViewState="False" ID="ImageRPBottomRightCorner" runat="server" SkinID="BottomRightCorner" /></td>
                        </tr></table>
                        <asp:ContentPlaceHolder id="phOnceContent" runat="server"></asp:ContentPlaceHolder>
